    {
      "number": 9,
      "id": "PR_kwDOJs5J2s5pvulj",
      "title": "Add mention of required base64url padding",
      "url": "https://github.com/ietf-wg-privacypass/draft-ietf-privacypass-auth-scheme-extensions/pull/9",
      "state": "MERGED",
      "author": "thibmeu",
      "authorAssociation": "CONTRIBUTOR",
      "assignees": [],
      "labels": [],
